{
    "rewrites": [
      {
        "source": "/gitlab/:data*",
        "destination": "/api/index?data=gitlab/:data*"
      },
      {
        "source": "/bitbucket/:data*",
        "destination": "/api/index?data=bitbucket/:data*"
      },
      {
        "source": "/codeberg/:data*",
        "destination": "/api/index?data=codeberg/:data*"
      },
      {
        "source": "/github/:data*",
        "destination": "/api/index?data=github/:data*"
      },
      {
        "source": "/api/index/:data*",
        "destination": "/api/index?data=:data*"
      },
      {
        "source": "/api/:data*",
        "destination": "/api/index?data=:data*"
      }
    ],
    "github": {
      "silent": true
    }
  }
